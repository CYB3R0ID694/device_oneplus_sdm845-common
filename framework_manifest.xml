        <version>2.1</version>
        <interface>
            <name>IMapper</name>
            <instance>default</instance>
        </interface>
        <fqname>@3.0::IMapper/default</fqname>
        <fqname>@4.0::IMapper/default</fqname>
    </hal>
</manifest>
